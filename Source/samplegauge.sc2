		This.Refresh()
		
	ENDPROC

	PROCEDURE pgfProperties.SFPage4.cboType.Init
		with This
			.AddItem('Left to right')
			.AddItem('Top to bottom')
			.AddItem('From top left')
			.AddItem('From top right')
		endwith
		
	ENDPROC

	PROCEDURE pgfProperties.SFPage4.cboType.InteractiveChange
		Thisform.oGauge.nBackGradientMode = This.ListIndex - 1
		Thisform.DrawGauge()
		
	ENDPROC

	PROCEDURE pgfProperties.SFPage4.cboType.Refresh
		This.Enabled   = This.Parent.chkGradient.Value
		This.ListIndex = Thisform.oGauge.nBackGradientMode + 1
		
	ENDPROC

	PROCEDURE pgfProperties.SFPage4.chkGradient.InteractiveChange
		dodefault()
		This.Parent.Refresh()
		Thisform.DrawGauge()
		
	ENDPROC

