		This.Enabled = not This.Parent.chkGradient.Value
		
	ENDPROC

	PROCEDURE pgfProperties.SFPage4.lblFromColor.Refresh
		This.Enabled = This.Parent.chkGradient.Value
		
	ENDPROC

	PROCEDURE pgfProperties.SFPage4.lblGradient.Click
		This.Parent.chkGradient.Value = not This.Parent.chkGradient.Value
		This.Parent.chkGradient.InteractiveChange()
		
	ENDPROC

	PROCEDURE pgfProperties.SFPage4.lblToColor.Refresh
		This.Enabled = This.Parent.chkGradient.Value
		
	ENDPROC

	PROCEDURE pgfProperties.SFPage4.lblType.Refresh
		This.Enabled = This.Parent.chkGradient.Value
		
	ENDPROC

	PROCEDURE pgfProperties.SFPage4.shpBackColor.Click
		dodefault()
		Thisform.oGauge.nBackColor2 = This.FillColor
		Thisform.DrawGauge()
		
	ENDPROC

	PROCEDURE pgfProperties.SFPage4.shpBackColor.Refresh
		dodefault()
		This.Enabled = not This.Parent.chkGradient.Value
		
	ENDPROC

	PROCEDURE pgfProperties.SFPage4.shpFromColor.Refresh
		dodefault()
		This.Enabled = This.Parent.chkGradient.Value
		
	ENDPROC

	PROCEDURE pgfProperties.SFPage4.shpToColor.Refresh
		This.Enabled = This.Parent.chkGradient.Value
		if not This.Enabled
			Thisform.oGauge.nBackColor2 = Thisform.oGauge.nBackColor
		endif not This.Enabled
		dodefault()
		
	ENDPROC

ENDDEFINE
