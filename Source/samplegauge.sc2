	PROCEDURE pgfProperties.Sfpage3.Activate
		This.Refresh()
		
	ENDPROC

	PROCEDURE pgfProperties.Sfpage3.cboLabelFactor.Init
		with This
			.AddItem('1')
			.AddItem('1,000')
			.AddItem('10,000')
			.AddItem('100,000')
			.AddItem('1,000,000')
			.AddItem('10,000,000')
			.AddItem('100,000,000')
			.AddItem('1,000,000,000')
		endwith
		
	ENDPROC

	PROCEDURE pgfProperties.Sfpage3.cboLabelFactor.InteractiveChange
		Thisform.oGauge.nLabelFactor = val(strtran(This.DisplayValue, ','))
		Thisform.DrawGauge()
		
	ENDPROC

	PROCEDURE pgfProperties.Sfpage3.cboLabelFactor.Refresh
		local lcFactor, ;
			lnI
		lcFactor = alltrim(transform(Thisform.oGauge.nLabelFactor, '9,999,999,999'))
		for lnI = 1 to This.ListCount
			if This.List[lnI] == lcFactor
				This.ListIndex = lnI
				exit
			endif This.List[lnI] == lcFactor
		next lnI
		
	ENDPROC

	PROCEDURE pgfProperties.Sfpage3.chkAdjustLabelSize.InteractiveChange
		Thisform.oGauge.lAdjustLabelSize = This.Value
		Thisform.DrawGauge()
		
	ENDPROC

	PROCEDURE pgfProperties.Sfpage3.ctrLabelFont.Refresh
		This.SetFont(Thisform.oGauge.cLabelFontName, Thisform.oGauge.nLabelFontSize, ;
			Thisform.oGauge.lLabelFontBold, Thisform.oGauge.lLabelFontItalic)
		
	ENDPROC

	PROCEDURE pgfProperties.Sfpage3.ctrLabelFont.updatefont
		dodefault()
		with Thisform.oGauge
			if .cLabelFontName <> This.cFontName or ;
				.nLabelFontSize <> This.nFontSize or ;
				.lLabelFontBold <> This.lFontBold or ;
				.lLabelFontItalic <> This.lFontItalic
				.cLabelFontName   = This.cFontName
				.nLabelFontSize   = This.nFontSize
				.lLabelFontBold   = This.lFontBold
				.lLabelFontItalic = This.lFontItalic
				Thisform.DrawGauge()
			endif .cLabelFontName <> This.cFontName ...
		endwith
		
	ENDPROC

